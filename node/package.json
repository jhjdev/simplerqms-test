{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/bin/www.js",
    "dev": "tsc --watch & node --watch ./dist/bin/www.js",
    "live": "npm run build && node ./dist/bin/www.js",
    "docker:start": "docker compose up",
    "docker:start:detached": "docker compose up -d",
    "docker:stop": "docker compose down",
    "docker:rebuild": "docker compose build --no-cache",
    "docker:logs": "docker compose logs -f",
    "docker:exec": "docker compose exec node ash",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "http-errors": "~2.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.3",
    "sequelize": "^6.37.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jade": "^0.0.34",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/glob": "^8.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
