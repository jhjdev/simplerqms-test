{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "NODE_OPTIONS=\"--loader @swc-node/register/esm\" node ./bin/www.ts",
    "dev": "NODE_OPTIONS=\"--loader @swc-node/register/esm\" node --watch ./bin/www.ts",
    "live": "NODE_OPTIONS=\"--loader @swc-node/register/esm\" node ./bin/www.ts",
    "docker:start": "docker compose up",
    "docker:start:detached": "docker compose up -d",
    "docker:stop": "docker compose down",
    "docker:rebuild": "docker compose up --build",
    "docker:logs": "docker compose logs -f",
    "docker:exec": "docker compose exec node ash"
  },
  "dependencies": {
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.4.0",
    "express": "^4.21.2",
    "http-errors": "~2.0.0",
    "morgan": "^1.10.0",
    "postgres": "^3.4.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.24",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
